{
    "name": "tomatophp/tomato-orders",
    "type": "library",
    "description": "full ordering and shipping system management with invoices templates for TomatoPHP",
    "keywords": [
        "php",
        "laravel",
        "ecommerce",
        "ordering",
        "shipping",
        "ecommerce",
        "tomatophp"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TomatoPHP\\TomatoOrders\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomatoPHP\\TomatoOrders\\TomatoOrdersServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "tomatophp/console-helpers": "^1.1",
        "tomatophp/tomato-crm": "^1.1",
        "tomatophp/tomato-settings": "^1.1",
        "tomatophp/tomato-products": "^1.0",
        "tomatophp/tomato-coupons": "^1.0",
        "tomatophp/tomato-translations": "^1.0",
        "tomatophp/tomato-admin": "^1.1"
    }
}
